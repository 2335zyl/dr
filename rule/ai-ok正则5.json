"rules":[
  /* 五维动态感知规则 */
  {"name":"♻️终极净化",
   "hosts":["*"],
   "regex":[
     // 时长+路径双验证
     "#EXTINF:(3|5|8|10|15)\\.\\d+,[\\s\\S]{1,200}?/(?:ads|banner)/\\d{6}/",
     
     // 动态加密特征
     "#EXT-X-KEY:.*?/v\\d+/adkey\\.key",
     
