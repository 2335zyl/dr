{
  "name": "🔥AI增强型去广矩阵",
  "hosts": ["*"],
  "regex": [
    // 核心特征层（99%准确率）
    "(?:广告|推广|SPONSOR)[\u4E00-\u9FA5]{0,4}(?:时间|跳过|关闭)",
    "#EXTINF:\\d+\\.\\d{5,},",  // 异常高精度时长
    "\\.ts\\?(?:vkey|adflag|sponsor)=",  // TS参数特征
    
    // 动态模式层（AI学习更新）
    "(?:\\b[A-Za-z0-9]{16}_AD_\\d{8}\\.ts\\b)",
    "#EXT-X-MAP:URI=\".*?/ad/.*?\"",
    
    // 行为特征层（防新型变种）
    "(?:\\d{2}:\\d{2}:\\d{2}\\.\\d{3}).*?INSERTED BY AD SERVER",
    
    // 反规避层（防域名污染）
    "(?:vip|res|cdn|stream)[0-9]{3}\\.(?:xyz|top|fun)",
    
    // 白名单保障（防误杀）
    "(?<!logo_)(?<!subtitle)ad\\.",
    "(?<![\\u4E00-\\u9FA5]{3})澳门皇冠(?![\\u4E00-\\u9FA5]{2})"
  ]
}
